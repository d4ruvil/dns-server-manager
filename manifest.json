{
    "manifest_version": 3,
    "name": "Vulnerability Checker",
    "version": "1.0",
    "description": "Checks for Server Version Disclosure, DNS Misconfiguration, and CORS vulnerabilities.",
    "permissions": ["activeTab", "tabs", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }